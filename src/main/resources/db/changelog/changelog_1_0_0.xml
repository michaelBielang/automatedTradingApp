<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">

    <changeSet author="michael.bielang" id="add_chart_data_vos">
        <createTable tableName="chart_data_vos">
            <column autoIncrement="true" name="id" type="INTEGER">
                <constraints nullable="false" primaryKey="true" primaryKeyName="person_pkey"/>
            </column>
            <column name="date" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="high" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="low" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="open" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="close" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="quoteVolume" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="weightedAverage" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
